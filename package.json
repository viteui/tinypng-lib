  "author": "",
  "license": "ISC",
  "dependencies": {
    "tinypng-lib-wasm": "^1.0.50"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^4.7.3",
    "wcrane-tool": "^1.0.35"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viteui/tinypng-lib.git"
  },
  "licenses": [
    {
      "type": "MIT",
